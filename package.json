{
  "name": "bank-kata",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "express": "^4.16.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "reactstrap": "^5.0.0-beta.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "tcomb": "^3.2.25",
    "ws": "^5.1.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "server": "node api/server.js",
    "start": "npm-run-all -p server start-js",
    "test-socket": "node src/testServer.js",
    "start-ui": "npm-run-all -p test-socket start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    },
    "/socket": {
      "target": "ws://localhost:8080",
      "ws": true,
      "pathRewrite": {
        "^/socket": "/"
      }
    }
  },
  "devDependencies": {
    "bufferutil": "^3.0.3",
    "utf-8-validate": "^4.0.0"
  }
}
